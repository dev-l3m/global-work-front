{
  "name": "tmp-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:vercel": "vite build",
    "build:analyze": "ANALYZE=true yarn run build",
    "preview": "vite preview",
    "audit:perf": "node scripts/audit-performance.js",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix --no-warn-ignored",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ],
    "vite.config.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@unhead/vue": "^2.1.4",
    "@vercel/analytics": "^1.6.1",
    "axios": "^1.13.5",
    "gray-matter": "^4.0.3",
    "markdown-it": "^14.1.1",
    "pinia": "^2.1.7",
    "vite-plugin-vuetify": "^2.0.0",
    "vue": "^3.5.24",
    "vue-router": "^4.3.0",
    "vuetify": "^3.5.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.7.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.4"
  }
}
